.navigationBar {
  width: 100%;
  height: 90rpx;
  background: #f7f7f7;
}
.navigationBar .wrapper {
  position: relative;
  top: 24%;
}
.navigationBar .wrapper .return {
  position: absolute;
  top: 0;
  left: 8rpx;
  width: 50rpx;
  height: 50rpx;
  z-index: 99;
}
.navigationBar .wrapper .return image {
  position: absolute;
  top: 35%;
  left: 50%;
  width: 20rpx;
  height: 32rpx;
  margin: -16rpx 0 0 -10rpx;
}
.navigationBar .wrapper .title {
  position: relative;
  text-align: center;
  font-size: 25rpx;
}
.store_map {
  width: 100%;
  height: 520rpx;
  transition: .3s all linear;
}
cover-view {
  position: fixed;
  top: 15%;
  left: 2%;
  width: 60rpx;
  height: 60rpx;
}
.default_box {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 600rpx;
  height: 400rpx;
  margin: -200rpx 0 0 -300rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.default_box .icon {
  width: 124rpx;
  height: 124rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.default_box .icon image {
  display: block;
  width: 100%;
  height: 100%;
}
.default_box .unShow {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  padding: 10rpx 0 20rpx 0;
}
.store_content {
  position: relative;
  width: 100%;
}
.store_content .close {
  position: absolute;
  width: 100%;
  height: 50rpx;
  color: #757575;
  font-size: 24rpx;
  background: #f7f7f7;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  -webkit-box-shadow: 0 1px 2px 1px #DDDDDD;
  /* for ios */
  box-shadow: 0 2rpx 4rpx 2rpx #DDDDDD;
  /* for Android */
  z-index: 99;
}
.store_content .close .line {
  position: relative;
  top: -20rpx;
  width: 31rpx;
  height: 4rpx;
  background: #a2a2a3;
}
.store_content .close text {
  position: relative;
  top: -5rpx;
}
.store_content .store_wrapper {
  width: 710rpx;
  margin: 0 auto;
  padding-top: 50rpx;
  overflow: scroll;
}
.store_content .store_wrapper .active {
  border: 4rpx solid #de997d;
}
.store_content .store_wrapper .box {
  -webkit-box-shadow: 0 1px 2px 1px #DDDDDD;
  /* for ios */
  box-shadow: 0 2rpx 4rpx 2rpx #DDDDDD;
  /* for Android */
  margin: 25rpx auto;
  border-radius: 25rpx;
}
.store_content .store_wrapper .store {
  position: relative;
  width: 99%;
  height: 234rpx;
  background: #f7f7f7;
  padding: 20rpx 35rpx 0 40rpx;
  box-sizing: border-box;
  letter-spacing: 3rpx;
}
.store_content .store_wrapper .store .title {
  font-size: 30rpx;
  margin-bottom: 8rpx;
}
.store_content .store_wrapper .store .title image {
  width: 28rpx;
  height: 28rpx;
  padding-right: 20rpx;
}
.store_content .store_wrapper .store .info {
  color: #3c3c3c;
}
.store_content .store_wrapper .store .info .left_box {
  display: inline-block;
  float: left;
  font-size: 24rpx;
  width: 425rpx;
  border-right: 2rpx solid #8f8f8f;
}
.store_content .store_wrapper .store .info .left_box image {
  padding-right: 20rpx;
}
.store_content .store_wrapper .store .info .left_box .address {
  width: 25rpx;
  height: 30rpx;
}
.store_content .store_wrapper .store .info .left_box .clock {
  width: 28rpx;
  height: 28rpx;
}
.store_content .store_wrapper .store .info .left_box > view {
  margin-bottom: 8rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  vertical-align: middle;
}
.store_content .store_wrapper .store .info .right_box {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  line-height: 25rpx;
  color: #787878;
  letter-spacing: 0rpx;
}
.store_content .store_wrapper .store .info .right_box .box_wrapper {
  margin: 0 auto;
}
.store_content .store_wrapper .store .info .right_box .box_wrapper .distance {
  width: 100%;
  margin-bottom: 10rpx;
}
.store_content .store_wrapper .store .info .right_box .box_wrapper .distance .overflow {
  max-width: 150rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.store_content .store_wrapper .store .info .right_box .box_wrapper .icon_box {
  width: 90%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  z-index: 2;
}
.store_content .store_wrapper .store .info .right_box .box_wrapper .icon_box image {
  width: 33rpx;
  height: 33rpx;
}
.store_content .store_wrapper .store .btn {
  width: 83rpx;
  padding-left: 25rpx;
  height: 39rpx;
}
.store_content .store_wrapper .store .btn image {
  display: block;
  width: 100%;
  height: 100%;
}
.store_content .store_wrapper .underline {
  color: #9e9e9e;
  font-size: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  letter-spacing: 3rpx;
}
.store_content .stretch {
  height: auto;
}
